{
  "name": "@your-company/explainability-ui",
  "version": "0.1.0",
  "description": "Lightweight TypeScript SDK and React hypergraph viewer for HIF graphs",
  "license": "Apache-2.0",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "dev": "tsup src/index.ts --format esm --dts --watch --sourcemap",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": ">=17 || >=18",
    "react-dom": ">=17 || >=18"
  },
  "dependencies": {
    "d3-force": "^3.0.0",
    "d3-selection": "^3.0.0",
    "d3-zoom": "^3.0.0"
  },
  "devDependencies": {
    "@types/d3-force": "^3.0.10",
    "@types/d3-selection": "^3.0.11",
    "@types/d3-zoom": "^3.0.8",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tsup": "^8.2.4",
    "typescript": "^5.6.3",
    "vite": "^5.3.0"
  }
}
